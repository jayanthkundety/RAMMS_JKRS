                                <local:EntryControl  x:Name="enctrlDivision"  Text="{Binding strDivision}" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default"  IsEnabled="False" />
                                <!--</fe:AdvancedFrame>-->
