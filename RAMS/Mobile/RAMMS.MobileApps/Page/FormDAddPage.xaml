                                                <Button  Grid.Row="1" BackgroundColor="Transparent" x:Name="btnLabourCommand" IsVisible="False"  Command="{Binding AddLabourCommand}" HeightRequest="45"  WidthRequest="45" Margin="-17,10,0,0" Grid.Column="1" VerticalOptions="Start" HorizontalOptions="Start"/>
