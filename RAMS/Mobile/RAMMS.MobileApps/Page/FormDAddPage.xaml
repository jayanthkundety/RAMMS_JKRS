                                <local:ExtendedPicker Title="Select Section" x:Name="sectionCodePicker"    ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"   HorizontalOptions="FillAndExpand" TextColor="Black"/>
                            </locals:FrameRenders>

                            <Label Text="Section Name" Grid.Row="0" Grid.Column="4"  FontSize="Medium"   FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />

                            <locals:FrameRenders Grid.Row="1"  Grid.Column="4" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >

                                <!--<fe:AdvancedFrame Grid.Row="1" Grid.Column="4" InnerBackground="{StaticResource DashboardbgColor}" OutlineColor="{StaticResource BoxviewColor}">-->
                                <local:EntryControl  Text="{Binding strsectionname}" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}" Keyboard="Default"  IsEnabled="False" />
                            </locals:FrameRenders>
                            <!--</fe:AdvancedFrame>-->

                            <Label Text="Name of Crew Supervisor:" Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="2"   FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />

                            <locals:FrameRenders Grid.Row="3"  Grid.Column="1" Grid.ColumnSpan="2" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >

