                                <local:ExtendedPicker Title="Select RMU" x:Name="RMUCodePicker"    ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"   HorizontalOptions="FillAndExpand" TextColor="Black"/>
                            </locals:FrameRenders>


                            <Label Text="RMU Name" Grid.Row="0" Grid.Column="2"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />


