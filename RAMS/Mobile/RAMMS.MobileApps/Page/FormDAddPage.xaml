                                <!--<fe:AdvancedFrame Grid.Row="1" Grid.Column="2" InnerBackground="{StaticResource DashboardbgColor}" OutlineColor="{StaticResource BoxviewColor}">-->
                                <local:EntryControl x:Name="ectrlRMUName" Text="{Binding strRMUCode}" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default"  IsEnabled="False" />
                            </locals:FrameRenders>

                            <!--</fe:AdvancedFrame>-->



                            <Label Text="Section Code" Grid.Row="0" Grid.Column="3"  FontSize="Medium"   FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />

